{
  "name": "f5.web",
  "version": "1.0.1",
  "description": "Angular 2 AOT ",
  "scripts": {
    "ngc": "rimraf ./aot && ngc -p ./tsconfig.aot.json",
    "webpack-lib": "webpack --progress --config webpack.lib.config.js",
    "webpack-jit": "webpack --progress --config webpack.jit.config.js",
    "webpack-aot": "webpack --progress --config webpack.aot.config.js --optimize-minimize",
    "jit": "npm run webpack-jit && webpack-dev-server --config webpack.jit.config.js --port 4201",
    "aot": "rimraf ./dist/aot && npm run ngc && npm run copy && npm run webpack-aot && webpack-dev-server --config webpack.aot.config.js --port 4202",
    "build-jit": "npm run webpack-jit",
    "build-aot": "npm run ngc && npm run webpack-aot",
    "demo": "rimraf ./dist && npm run build-jit && npm run build-aot",
    "copy": "ncp ./app/sub ./aot/app/sub"
  },
  "keywords": [
    "angular2",
    "aot",
    "typescript",
    "webpack"
  ],
  "authors": [
    "Wassim Chegham <github@wassimchegham.com>"
  ],
  "license": "MIT",
  "devDependencies": {
    "@angular/common": "^2.0.0",
    "@angular/compiler": "^2.0.0",
    "@angular/compiler-cli": "^0.6.2",
    "@angular/core": "^2.0.0",
    "@angular/http": "^2.2.0",
    "@angular/platform-browser": "^2.0.0",
    "@angular/platform-browser-dynamic": "^2.0.0",
    "@angular/platform-server": "^2.0.0",
    "@angular/router": "~3.0.1",
    "@types/chai": "^3.4.34",
    "@types/es6-shim": "0.0.31",
    "@types/lodash": "^4.14.37",
    "@types/node": "^6.0.45",
    "@types/requirejs": "^2.1.26",
    "angular2-ie9-shims": "^0.0.2",
    "angular2-load-children-loader": "^0.1.3",
    "angular2-template-loader": "^0.5.0",
    "awesome-typescript-loader": "^2.2.4",
    "compression-webpack-plugin": "^0.3.1",
    "copy-webpack-plugin": "^3.0.1",
    "core-js": "^2.4.1",
    "es6-promise": "^3.3.0",
    "es6-shim": "^0.35.1",
    "html-webpack-plugin": "^2.22.0",
    "http-server": "^0.9.0",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "^0.1.8",
    "rimraf": "^2.5.4",
    "rxjs": "^5.0.0-beta.12",
    "ts-loader": "^0.8.2",
    "typescript": "^2.0.2",
    "typings": "^1.3.3",
    "webpack": "2.1.0-beta.22",
    "webpack-dashboard": "^0.1.8",
    "webpack-dev-server": "^1.15.1",
    "webpack-merge": "^0.14.1",
    "zone.js": "^0.6.21"
  },
  "dependencies": {
    "@angular/compiler-cli": "^0.6.4",
    "@angular/forms": "^2.1.0",
    "@angular/platform-server": "^2.1.1",
    "@types/lodash": "^4.14.37",
    "awesome-typescript-loader": "^2.2.4",
    "lodash": "^4.16.4",
    "ng2-bootstrap": "^1.1.16",
    "rxjs": "^5.0.0-rc.1"
  }
}
